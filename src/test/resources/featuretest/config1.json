{
    "outputs": [
        {
            "name": "terminal_out",
            "target": "terminal",
            "level": "ALL",
            "patterns": {
                "default": "[{level}] ({tags}) [{thread}: {fullClass}.{method}] - {message}\n"
            }
        },
        {
            "name": "log_file",
            "target": "file",
            "level": "ALL",
            "patterns": {
                "default": "[{time yyyy:MM:dd HH:mm:ss:SSS}] | [{level}] [{tags}] | [{thread}: {fullClass}.{method}] ({file}:{lineNumber}) - {message}\n"
            }
        },
        {
            "name": "json_log_file",
            "target": "json",
            "level": "ALL"
        }
    ]
}